{
  "version": 2,
  "buildCommand": "cd backend && npm install && npm run build",
  "outputDirectory": "backend/dist",
  "functions": {
    "backend/api/index.ts": {
      "runtime": "@vercel/node@3.0.0",
      "includeFiles": "backend/src/**"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}
